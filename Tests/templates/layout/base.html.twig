<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}NoursTable tests{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bootstrap/dist/css/bootstrap.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('bootstrap-table/dist/bootstrap-table.min.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('font-awesome/css/font-awesome.css') }}" type="text/css" />
    {% endblock %}
</head>
<body>
{% block body %}
    {% block content %}{% endblock %}
{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('jquery/dist/jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap/dist/js/bootstrap.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap-table/dist/bootstrap-table.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bootstrap-table/dist/locale/bootstrap-table-fr-FR.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('TableDnD/dist/jquery.tablednd.js') }}"></script>
    <script type="text/javascript" src="{{ asset('jquery-form/jquery.form.js') }}"></script>
    <script type="text/javascript" src="{{ asset('object-path/index.js') }}"></script>
{% endblock %}
</body>
</html>
