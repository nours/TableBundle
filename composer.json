{
    "name": "nours/table-bundle",
    "type": "library",
    "description": "Table bundle for Symfony",
    "homepage": "https://github.com/nours/TableBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Coudrier",
            "email": "david.coudrier@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "^2.8|^3.3|^4.0",
        "doctrine/doctrine-bundle" : "~1.8",
        "psr/container": "~1.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "phpunit/phpunit" : "~4.0|~5.0",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/monolog-bundle" : "^3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/annotations": "~1.2",
        "doctrine/orm" : "~2.5",
        "jms/serializer-bundle" : "~2.3",
        "doctrine/data-fixtures": "~1.1",
        "beberlei/doctrineextensions": "^1.1",
        "twig/twig": "~2.0"
    },
    "suggest" : {
        "doctrine/orm": "Integrated using orm extension"
    },
    "autoload": {
        "psr-4" : {
            "Nours\\TableBundle\\" : ""
        }
    }
}
