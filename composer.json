{
    "name": "nours/table-bundle",
    "type": "library",
    "description": "Table implementation for Symfony2",
    "homepage": "https://github.com/nours/TableBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Coudrier",
            "email": "david.coudrier@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/options-resolver": ">2.6",
        "symfony/dependency-injection": ">2.6",
        "symfony/twig-bundle": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.1.*",
        "phpspec/prophecy-phpunit" : "~1.0",
        "symfony/symfony": "2.7.*",
        "symfony/monolog-bundle" : "~2.7",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/annotations": "~1.2",
        "pagerfanta/pagerfanta" : "~1.0",
        "doctrine/orm" : "~2.5",
        "doctrine/doctrine-bundle" : "~1.4",
        "jms/serializer-bundle" : "~0.13",
        "doctrine/data-fixtures": "~1.1"
    },
    "suggest" : {
        "pagerfanta/pagerfanta": "Integrated using pagerfanta extension",
        "doctrine/orm": "Integrated using orm extension"
    },
    "autoload": {
        "psr-4" : {
            "Nours\\TableBundle\\" : ""
        }
    }
}
